[user]
	name = John Webb
	email = John.Webb94@gmail.com
[core]
	editor = vim
[push]
	default = upstream
[includeIf "gitdir:~/"]
	path = ~/env.gitconfig
[fetch]
	prune = true
[commit]
	gpgsign = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[merge]
	tool = nvimdiff
